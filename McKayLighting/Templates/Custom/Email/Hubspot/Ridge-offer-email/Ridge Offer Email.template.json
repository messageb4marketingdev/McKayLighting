          "value" : "{% set sans_serif = \"Helvetica, Arial, Verdana, Trebuchet MS, sans-serif\" %}\n\n{% set serif = \"Georgia, Palatino, Times New Roman, Courier\" %}\n\n<style>\n    @import url('https://fonts.googleapis.com/css?family=Raleway:400,400i,700,700i,800,800i');\n    [style*=\"sans-serif\"] {\n        font-family: 'Raleway', Helvetica, Arial, Verdana, Trebuchet MS !important;\n    }\n    h3 {\n       font-family: {{ serif }};\n       font-size: 18px;\n       font-weight: bold; \n    }\n    p {\n        font-size: 12px;\n    }\n    body, body > table, #contentCell, .widget-type-email_view_as_web_page {\n        background-color: #efefef !important;\n    }\n    body #contentTableOuter {\n        box-shadow: none !important;\n        border: none !important;\n        border-bottom: none !important;\n        padding: 0 !important;\n    }\n    body #headerTable {\n        padding-right: 0px !important;\n    }\n    .widget-type-email_view_as_web_page {\n        padding-right: 20px;\n    }\n    .cta_button img {\n        font-size: 30px;\n        color: #fff;\n        background-color: #f06362;\n    }\n    .address a {\n        color: #a1aeac;\n    }\n    @media only screen and (max-width: 650px) {\n        #social-header {\n            margin: 0 auto;\n            float: none;\n        }\n        .left-image img {\n            max-width: 100% !important;\n            width: 100% !important;\n        }\n    }\n    @media only screen and (max-width: 480px) {\n        .social-media, .logo {\n            text-align: center !important;\n            padding-bottom: 10px !important;\n        }\n        td.mobile-padding {\n            padding-top: 20px !important;\n            padding-bottom: 20px !important;\n        }\n        body, body > table, #contentCell, .widget-type-email_view_as_web_page {\n            background-color: #ffffff !important;\n        }\n        .main-body {\n            padding-top: 10px;\n        }\n        td.header-content {\n            padding-top: 30px !important;\n        }\n    }\n</style>"
